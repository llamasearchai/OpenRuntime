# OpenRuntime Enhanced Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit the .env file to version control

# SECURITY - REQUIRED: Change ALL passwords before deployment!
POSTGRES_PASSWORD=CHANGE_THIS_SECURE_PASSWORD
REDIS_PASSWORD=CHANGE_THIS_SECURE_PASSWORD
GRAFANA_PASSWORD=CHANGE_THIS_SECURE_PASSWORD
JWT_SECRET=CHANGE_THIS_SECURE_SECRET_KEY

# Required: OpenAI API Key
OPENAI_API_KEY=sk-your-openai-api-key-here

# Optional: Other settings
LOG_LEVEL=info
MAX_CONCURRENT_TASKS=20
GPU_FALLBACK_TO_CPU=true
MLX_ENABLED=true

# Monitoring
PROMETHEUS_ENABLED=true
GRAFANA_USER=admin
